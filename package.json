{
    "name": "veggie-code",
    "displayName": "veggie-code",
    "description": "Help developers organize their code",
    "version": "1.0.0",
    "icon": "logo.png",
    "repository" : {
        "type": "git",
        "url": "https://github.com/BrinkOfMan/veggie-code"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Formatters",
        "Snippets"
    ],
    "extensionKind": [
		"ui"
	],
    "activationEvents": [
        "onLnaguage:c",
        "onLanguage:cpp",
        "onLanguage:python"
    ],
    "main": "./output/extension",
    "publisher": "Norman Burachara, Ethan Brinkman, Eric Johnson",
    "sync.ignoredExtensions": [],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
    
}
